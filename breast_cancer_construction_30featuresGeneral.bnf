<start> ::= classifier_name = <classifier>; buildFeatures = np.zeros([c, 1], dtype=float); buildFeatures[:,0] = <e>
			| classifier_name = <classifier>; buildFeatures = np.zeros([c, 2], dtype=float); buildFeatures[:,1] = <e>; buildFeatures[:,0] = <e>
			| classifier_name = <classifier>; buildFeatures = np.zeros([c, 3], dtype=float); buildFeatures[:,2] = <e>; buildFeatures[:,1] = <e>; buildFeatures[:,0] = <e>
			| classifier_name = <classifier>; buildFeatures = np.zeros([c, 4], dtype=float); buildFeatures[:,3] = <e>; buildFeatures[:,2] = <e>; buildFeatures[:,1] = <e>; buildFeatures[:,0] = <e>
			| classifier_name = <classifier>; buildFeatures = np.zeros([c, 5], dtype=float); buildFeatures[:,4] = <e>; buildFeatures[:,3] = <e>; buildFeatures[:,2] = <e>; buildFeatures[:,1] = <e>; buildFeatures[:,0] = <e>
			| classifier_name = <classifier>; buildFeatures = np.zeros([c, 6], dtype=float); buildFeatures[:,5] = <e>; buildFeatures[:,4] = <e>; buildFeatures[:,3] = <e>; buildFeatures[:,2] = <e>; buildFeatures[:,1] = <e>; buildFeatures[:,0] = <e>
